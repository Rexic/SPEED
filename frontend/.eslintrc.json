{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "react-app",
        "react-app/jest",
        "airbnb",
        "plugin:import/errors",
        "plugin:import/warnings",
        "plugin:import/typescript"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "react/jsx-uses-react": ["off"],
        "react/react-in-jsx-scope": ["off"],
        "react/jsx-props-no-spreading": ["warn"],
        "no-shadow": "off",
        "react/jsx-filename-extension": [0]
    }
}
